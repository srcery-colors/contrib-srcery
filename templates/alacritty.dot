# https://github.com/srcery-colors/srcery-terminal/

# Colors (Srcery)
colors:
  # Default colors
  primary:
    background: '{{=c.background}}'
    foreground: '{{=c.foreground}}'

  # Cursor colors
  cursor:
    text: CellBackground
    cursor: '{{=c.cursor}}'

  # Normal colors
  normal:
    black:   '{{=c[0]}}'
    red:     '{{=c[1]}}'
    green:   '{{=c[2]}}'
    yellow:  '{{=c[3]}}'
    blue:    '{{=c[4]}}'
    magenta: '{{=c[5]}}'
    cyan:    '{{=c[6]}}'
    white:   '{{=c[7]}}'

  # Bright colors
  bright:
    black:   '{{=c[8]}}'
    red:     '{{=c[9]}}'
    green:   '{{=c[10]}}'
    yellow:  '{{=c[11]}}'
    blue:    '{{=c[12]}}'
    magenta: '{{=c[13]}}'
    cyan:    '{{=c[14]}}'
    white:   '{{=c[15]}}'
